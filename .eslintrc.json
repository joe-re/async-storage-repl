{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "globals": {
    "_ayncStorageWebSocketServer": true,
    "WebSocket": true,
    "MessageEvent": true,
    "CloseEvent": true,
    "describe": true,
    "beforeEach": true,
    "context": true,
    "it": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "no-console": ["error", { "allow": ["info", "warn", "error"] }]
  }
}
